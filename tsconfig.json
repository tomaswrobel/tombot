{
	"compilerOptions": {
		"strict": true,
		"outDir": "dist",
		"module": "ESNext",
		"target": "ES2022",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"useUnknownInCatchVariables": true,
		"allowUnreachableCode": false,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"importHelpers": true,
		"typeRoots": ["node_modules/@types"],
		"types": ["node"],
		"strictPropertyInitialization": false
	}
}
